#include <iostream>
#include <cmath>

int main() {
    char a;
    a=0;
    for(int i=0; i<=255; i++);
    {std::cout<<a<<std::endl;
    a++;}
    std::cout<<"Enter symbols"<<std::endl
   char b;
    b=0;
    std::cin>>b;
    if((b<=57)&&(b>=48))
        std::cout<<"digit"<<std::endl;
    else if (((b>=65)&&(b<=90))||((b>=97)&&(b<=122)))
        std::cout<<"letter"<<std::endl;
    else std::cout<<"symbol unknown"<<std::endl;
    unsigned int c;
    std::cout<<"Enter x"<<std::endl;
    std::cin>>c;
    double q;
    int t=c%5;
    switch(t)
    { case 0: q= pow(c,2); break;
        case 1: q=c; break;
        case 2: q=0; break;
        case 3: q=-c; break;
        case 4: q=-1; break
    }
    std::cout<<q<<std::endl;
    int x;
    std::cout<<"Enter x"<<std::endl;
    double x;
    double y;
    std::cin>>x;
    int n;
    w=cos(x)=x+(pow(-1,n)*((pow(x,2n))/fact(2n));
    std::cout<<w<<std::endl;

    return 0;
}
